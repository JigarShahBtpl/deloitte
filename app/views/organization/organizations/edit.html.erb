<h2>Edit</h2>

<%= form_for(@organization) do |f| %>
  <%= render "users/shared/error_messages", resource: @organization %>

  <h4> Organization Details </h4>

  <div class="field">
    <%= f.label :organization_name %><br />
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </div>

  <div class="field">
    <%= f.label :phone_number %><br />
    <%= f.text_field :phone_number %>
  </div>

  <div class="field">
    <%= f.label :cin %><br />
    <%= f.text_field :cin %>
  </div>

  <div class="field">
    <%= f.label :pan %><br />
    <%= f.text_field :pan %>
  </div>

  <div class="field">
    <%= f.label :gst %><br />
    <%= f.text_field :gst %>
  </div>

  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_area :address %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>